{
    "name": "jinfeijie/packagist-mirror-cn",
    "description": "中国的packagist镜像composer包",
    "keywords": ["packagist", "mirror","mirror-cn"],
    "license": "MIT",
    "authors": [{
        "name": "jinfeijie",
        "email": "me@jinfeijie.cn"
    }],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "~6",
        "vlucas/phpdotenv": "~2",
        "symfony/console": "~3",
        "sebastian/version": "~2",
        "nesbot/carbon": "~1",
        "dariuszp/cli-progress-bar": "~1",
        "php-snippets/circular-array": "~1",
        "league/flysystem-cached-adapter": "~1",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "webysther/composer-plugin-qa": "~1",
        "webysther/composer-meta-qa": "~1",
        "league/flysystem-memory": "~1",
        "mikey179/vfsStream": "~1"
    },
    "suggest": {
        "hirak/prestissimo": "Composer parallel install plugin"
    },
    "autoload": {
        "psr-4": {
            "Webs\\Mirror\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webs\\Mirror\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/mirror"
    ]
}